<Window x:Class="LeeMusic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeeMusic"
        mc:Ignorable="d"
        Title="MainWindow" Height="119.604" Width="644.604">
    <Grid>
        <Button x:Name="playButton" Content="play" HorizontalAlignment="Left" Margin="3,52,0,0" VerticalAlignment="Top" Width="54" Height="27" Click="btnPlay"/>
        <Button x:Name="back" Content="back" HorizontalAlignment="Left" Margin="62,52,0,0" VerticalAlignment="Top" Width="54" Height="27" Click="btnBack"/>
        <Button x:Name="foward" Content="foward" HorizontalAlignment="Left" Margin="121,52,0,0" VerticalAlignment="Top" Width="54" Height="27" RenderTransformOrigin="0.691,1.538" Click="btnFoward"/>
        <Button x:Name="playList" Content="playList" HorizontalAlignment="Left" Margin="3,10,0,0" VerticalAlignment="Top" Width="54" Height="27" Click="readPlayList"/>
        <Button x:Name="button" Content="" HorizontalAlignment="Left" Margin="607,10,0,0" VerticalAlignment="Top" Width="20" Height="18" RenderTransformOrigin="2.871,2.043" Click="closeWindow"/>

    </Grid>
</Window>
